{
  "name": "scrolly",
  "version": "0.0.0",
  "description": "Scrolly",
  "author": {
    "name": "Trevor Blades",
    "email": "tdblades@gmail.com"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "gulp dev",
    "dist": "NODE_ENV=production gulp dist",
    "viewer": "NODE_ENV=viewer gulp viewer",
    "lint": "eslint src gulpfile.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "livereactload/babel-transform",
                  "imports": [
                    "react"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "planet/react"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.16.0",
    "browserify": "^13.1.0",
    "del": "^2.2.2",
    "envify": "^4.0.0",
    "eslint": "^3.5.0",
    "eslint-config-planet": "^9.0.0",
    "eslint-plugin-react": "^6.7.1",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-less": "^3.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "livereactload": "^3.1.1",
    "react-proxy": "^1.1.8",
    "run-sequence": "^1.2.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "bytes": "^2.4.0",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "mime": "^1.3.4",
    "moment": "^2.17.1",
    "planet-css": "^1.1.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-undo": "^0.6.1",
    "sentence-case": "^2.1.0",
    "uniqid": "^4.1.1",
    "upper-case-first": "^1.1.2",
    "whatwg-fetch": "^2.0.1"
  }
}
